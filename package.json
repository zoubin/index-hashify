{
  "name": "index-hashify",
  "version": "0.1.0",
  "description": "use sha1 index to  browserify row rather than integer",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint && tap test/*.js",
    "cov": "tap --cov test/*.js",
    "lint": "eslint *.js 'lib/**/*.js' test/*.js bin/*.js",
    "coveralls": "COVERALLS_REPO_TOKEN=REPO_TOKEN npm run cov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoubin/index-hashify.git"
  },
  "keywords": [
    "browserify-plugin",
    "hashify"
  ],
  "author": "zoubin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zoubin/index-hashify/issues"
  },
  "homepage": "https://github.com/zoubin/index-hashify#readme",
  "devDependencies": {
    "eslint": "^2.1.0",
    "tap": "^5.0.0"
  },
  "dependencies": {
    "shasum": "^1.0.2"
  }
}
